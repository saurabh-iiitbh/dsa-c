#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node *next;
};
struct node *newnode, *temp, *head;
void create(){
    head=0;
    for(int i=1;i<=5;i++){
        newnode=malloc(sizeof(struct node));
        printf("Enter data: \n");
        scanf("%d",&newnode->data);
        if(head==0){
            head=temp=newnode;
        }
        else{
            temp->next=newnode;
            temp=newnode;
            newnode->next=NULL;
        }
        
    }

        

    



}
void display(){
    temp=head;
   while(temp!=0){
    printf("%d->", temp->data);
    temp=temp->next;
   }
}
int main(){
    create();
    display();
}
